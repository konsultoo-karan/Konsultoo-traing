<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_uom__ksc_tree" model="ir.ui.view">
        <field name="name">product.uom.ksc.tree</field>
        <field name="model">product.uom.ksc</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="uom_category_id"/>
            </tree>
        </field>
    </record>

    <record id="product_uom_ksc_form" model="ir.ui.view">
        <field name="name">product.uom.ksc.form</field>
        <field name="model">product.uom.ksc</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="uom_category_id"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="product_uom_ksc_search" model="ir.ui.view">
        <field name="name">product uom ksc search</field>
        <field name="model">product.uom.ksc</field>
        <field name="arch" type="xml">
            <search string="Search Uom category">
                <field name="name"/>
            </search>
        </field>
    </record>


    <record model="ir.actions.act_window" id="action_product_uom_ksc">
        <field name="name">product uom ksc</field>
        <field name="res_model">product.uom.ksc</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="menu_product_uom_ksc"  sequence="2"/>
    <menuitem id="menu_product_uom_ksc_2" name="uom" parent="product_category_ksc_id_1"
              action="action_product_uom_ksc"
              sequence="4"/>

</odoo>